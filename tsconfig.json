{
  "compilerOptions": {
    // "target": "ES5",
    "target": "es6",
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "outDir": "lib",
    "paths": {
      "@/*": ["src/*"],
      "@utils/*":["src/utils/*"]
    },
    "allowSyntheticDefaultImports": true,
  }
}
